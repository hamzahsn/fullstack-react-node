{
  "name": "typescript-boilerplate",
  "version": "1.0.0",
  "description": "Full-stack sample application build with webpack, React, NodeJs, Typescript and MongoDB",
  "main": "index.js",
  "private": true,
  "engines": {
    "node": "12.18.3"
  },
  "scripts": {
    "format": "prettier --write src/**/*.{ts,tsx}",
    "stylelint": "stylelint 'src/client/**/*.{css,scss}' --fix",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "lint-fix": "npm run lint -- --fix",
    "build-client": "cross-env NODE_ENV=production webpack",
    "dev-client": "cross-env TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\" NODE_ENV=development webpack serve "
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{css.scss}": [
      "npm run stylelint -- --fix"
    ],
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --cache --fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hamzahsn/fullstack-react-node.git"
  },
  "keywords": [
    "typescript",
    "react",
    "webpack",
    "fullstack",
    "express",
    "node",
    "mongodb"
  ],
  "author": {
    "name": "Hamza Amdouni",
    "email": "amdouni.hamza.hsn@gmail.com",
    "url": "https://iamhamza.io"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hamzahsn/fullstack-react-node/issues",
    "email": "amdouni.hamza.hsn@gmail.com"
  },
  "homepage": "https://github.com/hamzahsn/fullstack-react-node#readme",
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.10.9",
    "react": "^16.14.0",
    "react-dom": "^16.14.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@babel/plugin-proposal-numeric-separator": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.8",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.8",
    "@types/cssnano": "^4.0.0",
    "@types/debug": "^4.1.5",
    "@types/enzyme": "^3.10.7",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/express": "^4.17.8",
    "@types/fork-ts-checker-webpack-plugin": "^0.4.5",
    "@types/jest": "^26.0.14",
    "@types/mongoose": "^5.7.36",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.6",
    "@types/webpack": "^4.41.22",
    "@types/webpack-bundle-analyzer": "^3.8.0",
    "@types/webpack-dev-server": "^3.11.0",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "babel-loader": "^8.1.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "concurrently": "^5.3.0",
    "core-js": "^3.6.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "css-loader": "^5.0.0",
    "cssnano": "^4.1.10",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "enzyme-to-json": "^3.6.1",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.4",
    "file-loader": "^6.1.1",
    "fork-ts-checker-webpack-plugin": "^5.2.0",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^4.3.0",
    "jest": "^26.5.3",
    "lint-staged": "^10.4.1",
    "postcss": "^8.1.1",
    "postcss-loader": "^4.0.4",
    "prettier": "^2.1.2",
    "style-loader": "^2.0.0",
    "stylelint": "^13.7.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "ts-jest": "^26.4.1",
    "ts-node": "^9.0.0",
